{
    "name": "my-monorepo-project",
    "version": "1.0.0",
    "private": true,
    "workspaces": [
        "app",
        "server"
    ],
    "scripts": {
        "dev": "concurrently \"npm:start --workspace=server\" \"npm:dev --workspace=app\"",
        "build": "npm run build --workspace=app"
    },
    "devDependencies": {
        "concurrently": "^9.1.2"
    }
}