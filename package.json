{
    "name": "whybot-ai-mindmap",
    "version": "1.0.0",
    "private": true,
    "workspaces": [
        "app",
        "server"
    ],
    "scripts": {
        "dev": "concurrently \"npm:start --workspace=server\" \"npm:dev --workspace=app\"",
        "build": "npm run build --workspace=app",
        "lint": "npm run lint --workspace=app && npm run lint --workspace=server",
        "lint:app": "npm run lint --workspace=app",
        "lint:server": "npm run lint --workspace=server",
        "lint:app:fix": "npm run lint:fix --workspace=app",
        "lint:server:fix": "npm run lint:fix --workspace=server",
        "format:app": "npm run format --workspace=app",
        "format:server": "npm run format --workspace=server",
        "compile:app": "npx tsc --noEmit -p app/tsconfig.json",
        "clean:app": "rm -Rf app/node_modules",
        "clean:server": "rm -Rf server/node_modules",
        "clean:monorepo": "rm -Rf node_modules package-lock.json",
        "clean:all": "rm -Rf app/node_modules && rm -Rf server/node_modules && rm -Rf node_modules package-lock.json"
    },
    "devDependencies": {
        "concurrently": "^9.1.2"
    },
    "dependencies": {
        "@reactflow/node-resizer": "^2.2.14",
        "@xyflow/react": "^12.5.6",
        "entitree-flex": "^0.4.1",
        "puppeteer": "^24.6.1"
    }
}